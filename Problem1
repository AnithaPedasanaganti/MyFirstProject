public class Example() //Creation of class
{
 public static void main(String [] args)  
 {  
  Example obj = new Example();  //Creating an object of the class 
  System.out.println("The sum is: "+obj.add(12, 13));  
 }  
int s;   
 public int add(int a, int b)  //Creating an user defined method and its Signature
 {  
  s = a+b;  
  return s;  
 }  
}  
